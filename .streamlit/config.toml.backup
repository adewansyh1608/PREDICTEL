[theme]
primaryColor = "#0ea5e9"
backgroundColor = "#0a0a0a"
secondaryBackgroundColor = "#111111"
textColor = "#ffffff"
font = "sans serif"

[server]
headless = true
port = 8501
enableXsrfProtection = false
maxUploadSize = 200

[browser]
gatherUsageStats = false

[runner]
magicEnabled = true

[logger]
level = "info"
